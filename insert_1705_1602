-- FUNCTION: owd.insert_1705_1602(date, date)

-- DROP FUNCTION IF EXISTS owd.insert_1705_1602(date, date);

CREATE OR REPLACE FUNCTION owd.insert_1705_1602(
	fecha_inicio date,
	fecha_fin date)
    RETURNS text
    LANGUAGE 'plpgsql'
    COST 100
    VOLATILE PARALLEL UNSAFE
AS $BODY$
DECLARE 
	punto record;	

	codigoproceso smallint;
	codigoarchivo smallint;
	rut character varying(20);
    periodo integer;
	
    codigo_obra character varying(255); --1
	lifecyclestatus integer; --2
	tipo integer; --3
	potencia_subestacion numeric(38,2); --4
	ownedby smallint;
	
	gdb_from_date  timestamp without time zone;
	objectid integer;
	
    codigo_obra2 character varying(255); --1
	lifecyclestatus2 integer; --2
	tipo2 integer; --3
	potencia_subestacion2 numeric(38,2); --4
	ownedby2 smallint;
	
	gdb_from_date2  timestamp without time zone;
	objectid2 integer;	
	
	gdb_archive_oid integer;
	gdb_archive_oid2 integer;
	proyecto_informado_siss character varying(100);
	proyecto_informado_siss2 character varying(100);
	resultado text;
	cantidad integer;
	estado smallint;

	cuenta integer;
BEGIN

codigoproceso :=  5;
codigoarchivo := 1705; 
--rut := '96963440';
periodo := ((date_part('year'::text, CURRENT_DATE) - 1::double precision) || '12')::integer;

--resultado   := '';
estado := 0;
cantidad := 0;

cuenta := 0;
--REFRESH MATERIALIZED VIEW owd.vmt_1705_dev;
--DELETE FROM owd.tabla_1705 where fecha >= fecha_inicio AND fecha <= fecha_fin;

-----------------------------------------351---------------------------------------------------

  -- WHERE wd.assetgroup = 3 AND wd.assettype = 205;
  
SELECT count(*) into cantidad
	FROM owd.vmt_1705_subestaciones_dbl vmt WHERE  vmt.objectid  in 
(select distinct vmt_1.objectid from owd.vmt_1705_subestaciones_dbl vmt_1 WHERE vmt_1.created_date < fecha_inicio
AND vmt_1.gdb_from_date BETWEEN fecha_inicio AND fecha_fin);
	 
if cantidad > 0 then
   	FOR punto IN SELECT * 
	FROM owd.vmt_1705_subestaciones_dbl vmt WHERE vmt.objectid  in 
	(select distinct vmt_1.objectid from owd.vmt_1705_subestaciones_dbl vmt_1 WHERE vmt_1.created_date < fecha_inicio
	AND vmt_1.gdb_from_date BETWEEN fecha_inicio AND fecha_fin) 
	order by vmt.ownedby, vmt.objectid, vmt.gdb_from_date LOOP
		cuenta := cuenta + 1;
		
		--resultado := resultado || '_//_cuenta_' || cuenta ;

		IF cuenta = 1 THEN

			estado := 1;
			
			codigo_obra := punto.codigo_obra; --1
			lifecyclestatus := punto.lifecyclestatus; --2
			tipo := punto.tipo; --3
			potencia_subestacion := punto.potencia_subestacion; --4

			objectid = punto.objectid;
			
			gdb_archive_oid := punto.gdb_archive_oid;
			gdb_from_date := punto.gdb_from_date;
			proyecto_informado_siss := punto.proyecto_informado_siss;
			--cuenta = punto.cuenta;
			ownedby := punto.ownedby;
			
		ELSIF objectid = punto.objectid AND punto.gdb_from_date < fecha_inicio AND estado = 1 THEN			
			codigo_obra := punto.codigo_obra; --1
			lifecyclestatus := punto.lifecyclestatus; --2
			tipo := punto.tipo; --3
			potencia_subestacion := punto.potencia_subestacion; --4

			objectid = punto.objectid;
			
			gdb_archive_oid := punto.gdb_archive_oid;
			gdb_from_date := punto.gdb_from_date;
			proyecto_informado_siss := punto.proyecto_informado_siss;
			--cuenta = punto.cuenta;
			ownedby := punto.ownedby;			
			
		ELSIF punto.gdb_from_date > fecha_inicio AND estado = 1 AND objectid = punto.objectid THEN

			estado := 2;
			
			codigo_obra2 := punto.codigo_obra; --1
			lifecyclestatus2 := punto.lifecyclestatus; --2
			tipo2 := punto.tipo; --3
			potencia_subestacion2 := punto.potencia_subestacion; --4


			objectid2 = punto.objectid;

			gdb_archive_oid2 := punto.gdb_archive_oid;
			gdb_from_date2 := punto.gdb_from_date;
			proyecto_informado_siss2 := punto.proyecto_informado_siss;	
			ownedby2 := punto.ownedby;
				
		END IF;
		
		IF punto.gdb_from_date < fecha_fin AND objectid = punto.objectid AND estado = 2 THEN
			
			IF cuenta = cantidad THEN		
				IF ownedby = 103 THEN rut = '76833300';
				ELSIF ownedby = 104 THEN rut = '96963440';
				ELSE rut = NULL;
				END IF;		
				/*IF  COALESCE(codigo_obra ,'0')  <>  COALESCE(punto.codigo_obra,'0') THEN
					INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
					VALUES (codigoproceso, codigoarchivo, rut, periodo, punto.proyecto_informado_siss, punto.codigo_sistema_siss, 1602, punto.codigo_obra, '0', 1, codigo_obra::text, punto.codigo_obra::text, punto.gdb_from_date, punto.assetid::integer);
				END IF;	*/
				IF  COALESCE(lifecyclestatus,'0')  <>  COALESCE(punto.lifecyclestatus,'0') THEN
					INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
					VALUES (codigoproceso, codigoarchivo, rut, periodo, punto.proyecto_informado_siss, punto.codigo_sistema_siss, 1602, punto.codigo_obra, '0', 2, lifecyclestatus::text, punto.lifecyclestatus::text, punto.gdb_from_date, punto.objectid::integer);
				END IF;	
				IF  COALESCE(tipo,'0')  <>  COALESCE(punto.tipo,'0') THEN
					INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
					VALUES (codigoproceso, codigoarchivo, rut, periodo, punto.proyecto_informado_siss, punto.codigo_sistema_siss, 1602, punto.codigo_obra, '0', 3, tipo::text, punto.tipo::text, punto.gdb_from_date, punto.objectid::integer);
				END IF;	
				IF  COALESCE(potencia_subestacion,'0')  <>  COALESCE(punto.potencia_subestacion,'0') THEN
					INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
					VALUES (codigoproceso, codigoarchivo, rut, periodo, punto.proyecto_informado_siss, punto.codigo_sistema_siss, 1602, punto.codigo_obra, '0', 4, potencia_subestacion::text, round(punto.potencia_subestacion,2)::text, punto.gdb_from_date, punto.objectid::integer);
				END IF;		
		
			ELSE		
		
				codigo_obra2 := punto.codigo_obra; --1
				lifecyclestatus2 := punto.lifecyclestatus; --2
				tipo2 := punto.tipo; --3
				potencia_subestacion2 := punto.potencia_subestacion; --4


				objectid2 = punto.objectid;

				gdb_archive_oid2 := punto.gdb_archive_oid;
				gdb_from_date2 := punto.gdb_from_date;
				proyecto_informado_siss2 := punto.proyecto_informado_siss;	
				ownedby2 := punto.ownedby;		
			END IF;		
			
		ELSIF (punto.gdb_from_date > fecha_fin OR objectid <> punto.objectid)  AND estado = 2 THEN			

			IF ownedby = 103 THEN rut = '76833300';
			ELSIF ownedby = 104 THEN rut = '96963440';
			ELSE rut = NULL;
			END IF;				

			/*IF  COALESCE(codigo_obra,'0')  <>  COALESCE(codigo_obra2,'0') THEN
				INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
				VALUES (codigoproceso, codigoarchivo, rut, periodo, proyecto_informado_siss2, codigo_sistema_siss2, 1602, codigo_obra2, '0', 1, codigo_obra::text, codigo_obra2::text, gdb_from_date2, assetid2::integer);
			END IF;	*/
			IF  COALESCE(lifecyclestatus,'0')  <>  COALESCE(lifecyclestatus2,'0') THEN
				INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
				VALUES (codigoproceso, codigoarchivo, rut, periodo, proyecto_informado_siss2, codigo_sistema_siss2, 1602, codigo_obra2, '0', 2, lifecyclestatus::text, lifecyclestatus2::text, gdb_from_date2, objectid2::integer);
			END IF;			
			IF  COALESCE(tipo,'0')  <>  COALESCE(tipo2,'0') THEN
				INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
				VALUES (codigoproceso, codigoarchivo, rut, periodo, proyecto_informado_siss2, codigo_sistema_siss2, 1602, codigo_obra2, '0', 3, tipo::text, tipo2::text, gdb_from_date2, objectid2::integer);
			END IF;					
			IF  COALESCE(potencia_subestacion,'0')  <>  COALESCE(potencia_subestacion2,'0') THEN
				INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
				VALUES (codigoproceso, codigoarchivo, rut, periodo, proyecto_informado_siss2, codigo_sistema_siss2, 1602, codigo_obra2, '0', 4, potencia_subestacion::text, potencia_subestacion2::text, gdb_from_date2, objectid2::integer);
			END IF;					

			IF objectid = punto.objectid THEN

				estado := 3;

			ELSE
				estado := 1;
				
				codigo_obra := punto.codigo_obra; --1
				lifecyclestatus := punto.lifecyclestatus; --2
				tipo := punto.tipo; --3
				potencia_subestacion := punto.potencia_subestacion; --4

				objectid = punto.objectid;

				gdb_archive_oid := punto.gdb_archive_oid;
				gdb_from_date := punto.gdb_from_date;
				proyecto_informado_siss := punto.proyecto_informado_siss;
				--cuenta = punto.cuenta;
				ownedby := punto.ownedby;					
				
			END IF;				
		ELSIF (estado = 3 or estado = 1) AND punto.objectid <> objectid THEN

			estado := 1;

			codigo_obra := punto.codigo_obra; --1
			lifecyclestatus := punto.lifecyclestatus; --2
			tipo := punto.tipo; --3
			potencia_subestacion := punto.potencia_subestacion; --4

			objectid = punto.objectid;

			gdb_archive_oid := punto.gdb_archive_oid;
			gdb_from_date := punto.gdb_from_date;
			proyecto_informado_siss := punto.proyecto_informado_siss;
			--cuenta = punto.cuenta;
			ownedby := punto.ownedby;		

		END IF;
	END LOOP;
END IF;			

 resultado := cantidad::text;
 RETURN resultado;
END;
$BODY$;

ALTER FUNCTION owd.insert_1705_1602(date, date)
    OWNER TO owd;
