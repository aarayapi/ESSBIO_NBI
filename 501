SELECT 
wd.objectid, 
wd.assetgroup, 
wd.assettype, 
wd.globalid,
wd.gdb_branch_id, 
wd.gdb_from_date, 
wd.gdb_is_delete, 
wd.gdb_deleted_at, 
wd.gdb_deleted_by, 
wd.gdb_archive_oid
  FROM owd.waterdevice wd --LEFT JOIN  vmt_ap_plantatratamiento_sb ap ON wd.globalid::text = ap.assetguid::text   
  WHERE wd.gdb_branch_id = 0 AND wd.objectid in (select distinct wd1.objectid from waterdevice wd1 where wd1.creationdate != wd1.lastupdate)
  AND wd.assettype = 601 AND wd.assetgroup = 15