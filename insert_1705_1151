-- FUNCTION: owd.insert_1705_1151(timestamp without time zone, timestamp without time zone)

-- DROP FUNCTION IF EXISTS owd.insert_1705_1151(timestamp without time zone, timestamp without time zone);

CREATE OR REPLACE FUNCTION owd.insert_1705_1151(
	fecha_inicio timestamp without time zone,
	fecha_fin timestamp without time zone)
    RETURNS text
    LANGUAGE 'plpgsql'
    COST 100
    VOLATILE PARALLEL UNSAFE
AS $BODY$
DECLARE 
	--punto owd.vmt_sewerline_sb%ROWTYPE;	
	punto record;
	punto2 record;	
	--punto3 record;
	
	largo_total numeric;
	largo_tramo_hist numeric;
	diferencia numeric;
	largo_cond_hist numeric;
	largo_cond_hist2 numeric;
	largo_tramo numeric;

	codigoproceso smallint;
	codigoarchivo smallint;
	rut character varying(20);
    periodo integer;

    codigo_sistema_siss integer;
    codigo_obra character varying(255);	
    etapa integer;	
    name character varying(255);
	tipo_obra_origen integer;
	codigo_obra_origen  character varying(255);
	tipo_obra_destino integer;
	codigo_obra_destino character varying(255);
	--longitud_tramo numeric(38,8);
	assetid character varying(100);
	id_tramo_conectado character varying(20);
	ownedby smallint;
	objectid integer;
	
    codigo_sistema_siss2 integer;
    codigo_obra2 character varying(255);	
    etapa2 integer;	
    name2 character varying(255);
	tipo_obra_origen2 integer;
	codigo_obra_origen2  character varying(255);
	tipo_obra_destino2 integer;
	codigo_obra_destino2 character varying(255);
	--longitud_tramo2 numeric(38,8);
	--longitud_total2 numeric(38,8);
	assetid2 character varying(100);
	ownedby2 smallint;
	rut2 character varying(20); 
	
	
  	gdb_from_date timestamp;
	gdb_from_date2 timestamp;
	longitud_total numeric(38,8);
	gdb_archive_oid integer;
	gdb_archive_oid2 integer;
	proyecto_informado_siss character varying(100);
	proyecto_informado_siss2 character varying(100);
	resultado text;
	cantidad integer;
	correlativo integer;
	cuenta2 integer;
BEGIN
codigoproceso :=  5;
codigoarchivo := 1705; 
--rut := '96963440';
periodo := ((date_part('year'::text, CURRENT_DATE) - 1::double precision) || '12')::integer;

resultado   := '';
correlativo := 0;
cantidad := 0;
cuenta2 := 0;
longitud_total := 0;

TRUNCATE TABLE owd.tbl_tmp_1705;

FOR punto IN 
SELECT sl.proyecto_informado_siss,  sl.codigo_obra , sl.id_tramo_conectado,  sl.creationdate, 
	   sl.rut, sl.measuredlength, sl.objectid, sl.gdb_from_date, sl.gdb_archive_oid, sl.lifecyclestatus, sl.codigo_sistema_siss
FROM owd.vmt_1705_1151_sewerline sl WHERE 
	(sl.assetgroup = 1 OR sl.assettype = 2 OR sl.assettype = 41 OR sl.assettype = 42) 
	AND sl.creationdate >= fecha_inicio AND sl.creationdate <= fecha_fin
	AND sl.proyecto_informado_siss IS NOT null
	ORDER BY sl.rut, sl.codigo_obra, sl.gdb_from_date ASC	

	LOOP
		correlativo := correlativo + 1;


		IF correlativo = 1 THEN
			codigo_obra := punto.codigo_obra;
			proyecto_informado_siss := punto.proyecto_informado_siss;
			codigo_sistema_siss := punto.codigo_sistema_siss;
			rut := punto.rut;
			--longitud_total = longitud_total + punto.measuredlength;	
			SELECT longitud into longitud_total FROM owd.tbl_1151_hist WHERE codigo_conduccion = codigo_obra AND rut_empresa = rut;
			
			IF longitud_total IS NOT NULL AND punto.measuredlength IS NOT NULL THEN
			
				INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
				VALUES (codigoproceso, codigoarchivo, rut, periodo, punto.proyecto_informado_siss, punto.codigo_sistema_siss, 1151, punto.codigo_obra, 0, 9, round(longitud_total,2)::text, round(longitud_total + punto.measuredlength,2)::text, punto.gdb_from_date, punto.objectid::integer);
			
				longitud_total = longitud_total + punto.measuredlength;			
			
			END IF;
			
		ELSIF correlativo >= 2 THEN			
			codigo_obra2 := punto.codigo_obra;	
			rut2 := punto.rut;
			
			IF codigo_obra = codigo_obra2 AND rut = rut2 THEN		
				
				IF longitud_total IS NOT NULL AND punto.measuredlength IS NOT NULL THEN

					/*INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
					VALUES (codigoproceso, codigoarchivo, rut, periodo, punto.proyecto_informado_siss, punto.codigo_sistema_siss, 1151, punto.codigo_obra, 0, 9, round(longitud_total,2)::text, round(longitud_total + punto.measuredlength, 2)::text, punto.gdb_from_date, punto.objectid::integer);*/

					longitud_total = longitud_total + punto.measuredlength;

				END IF;

			ELSE
				IF longitud_total IS NOT NULL AND punto.measuredlength IS NOT NULL THEN
					INSERT INTO owd.tbl_tmp_1705(rut_empresa, codigo_conduccion, longitud) VALUES (rut, codigo_obra, longitud_total);
				END IF;




				codigo_obra := punto.codigo_obra;
				proyecto_informado_siss := punto.proyecto_informado_siss;
				codigo_sistema_siss := punto.codigo_sistema_siss;					
				rut := punto.rut;
				correlativo := 1;
				
				SELECT longitud into longitud_total FROM owd.tbl_1151_hist WHERE codigo_conduccion = codigo_obra AND rut_empresa = rut;
				
				IF longitud_total IS NOT NULL AND punto.measuredlength IS NOT NULL THEN

					INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
					VALUES (codigoproceso, codigoarchivo, rut, periodo, punto.proyecto_informado_siss, punto.codigo_sistema_siss, 1151, punto.codigo_obra, 0, 9, round(longitud_total,2)::text, round(longitud_total + punto.measuredlength,2)::text, punto.gdb_from_date, punto.objectid::integer);

					longitud_total := longitud_total + punto.measuredlength;
					
				END IF;									
			END IF;
		END IF;				
	END LOOP;
	
	correlativo = 0;
	largo_total = 0;
----------------------------------------------------------------------o---------------------------------------------------------------------------	
FOR punto IN 
	SELECT * FROM owd.vmt_1705_1151_sewerline_2 sl WHERE 
		(sl.assetgroup = 1 OR sl.assettype = 2 OR sl.assettype = 41 OR sl.assettype = 42) 
		AND sl.creationdate <= fecha_inicio AND sl.lastupdate BETWEEN fecha_inicio AND fecha_fin
		AND sl.proyecto_informado_siss IS NOT null
		ORDER BY sl.rut, sl.codigo_obra, sl.gdb_from_date ASC	
		
		LOOP	

			correlativo := correlativo + 1;

			IF correlativo = 1 THEN	
			
				SELECT TH.longitud INTO
				largo_tramo_hist FROM owd.tbl_1152_hist TH 
				WHERE TH.codigo_conduccion = punto.codigo_obra 
				AND TH.codigo_tramo = punto.id_tramo_conectado AND TH.rut_empresa = punto.rut;			
			
				SELECT TT.longitud INTO largo_cond_hist 
				FROM owd.tbl_tmp_1705 TT
				WHERE TT.rut_empresa = punto.rut AND TT.codigo_conduccion = punto.codigo_obra;

				SELECT longitud INTO largo_cond_hist2
				FROM owd.tbl_1151_hist TH 
				WHERE TH.codigo_conduccion = punto.codigo_obra AND TH.rut_empresa = punto.rut;	
				
				IF largo_tramo_hist IS NOT NULL AND punto.measuredlength IS NOT NULL THEN
					diferencia := round(punto.measuredlength, 2) - round(largo_tramo_hist, 2);
					IF diferencia <> 0 AND punto.measuredlength IS NOT NULL AND largo_tramo_hist IS NOT NULL THEN						
						
						IF largo_cond_hist IS NOT NULL THEN
							
							largo_total  := largo_cond_hist + diferencia;
							
							INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
							VALUES (codigoproceso, codigoarchivo, rut, periodo, punto.proyecto_informado_siss, punto.codigo_sistema_siss, 1151, punto.codigo_obra, 0, 9, round(largo_cond_hist,2)::text, round(largo_cond_hist + diferencia,2)::text, punto.gdb_from_date, punto.objectid::integer);
						
						ELSE
							
							largo_total  := largo_cond_hist2 + diferencia;

							INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
							VALUES (codigoproceso, codigoarchivo, rut, periodo, punto.proyecto_informado_siss, punto.codigo_sistema_siss, 1151, punto.codigo_obra, 0, 9, round(largo_cond_hist2,2)::text, round(largo_cond_hist2 + diferencia,2)::text, punto.gdb_from_date, punto.objectid::integer);						

						END IF;
					END IF;
				END IF;

				correlativo := correlativo + 1;
				codigo_sistema_siss := punto.codigo_sistema_siss;
				codigo_obra := punto.codigo_obra;
				etapa := punto.etapa;
				name := punto.nombre_conduccion;
				tipo_obra_origen:= punto.tipo_obra_origen;
				codigo_obra_origen := punto.codigo_obra_origen;
				tipo_obra_destino:= punto.tipo_obra_destino;
				codigo_obra_destino	:=	punto.codigo_obra_destino;	
				id_tramo_conectado := punto.id_tramo_conectado;
				largo_tramo := punto.measuredlength;
				rut := punto.rut;

				gdb_archive_oid := punto.gdb_archive_oid;
				gdb_from_date := punto.gdb_from_date;
				proyecto_informado_siss := punto.proyecto_informado_siss;
				assetid := punto.assetid;
				gdb_from_date = punto.gdb_from_date;	
				
			ELSE
				codigo_obra2 := punto.codigo_obra;
				rut2 := punto.rut;

				IF codigo_obra = codigo_obra2 AND rut = rut2 THEN		
					--correlativo = 1;
					IF largo_tramo_hist IS NOT NULL AND largo_tramo IS NOT NULL THEN
						diferencia :=  punto.measuredlength - largo_tramo_hist;		
						
						IF diferencia <> 0 AND punto.measuredlength IS NOT NULL AND largo_tramo_hist IS NOT NULL THEN	
						
							INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
							VALUES (codigoproceso, codigoarchivo, rut, periodo, punto.proyecto_informado_siss, punto.codigo_sistema_siss, 1151, punto.codigo_obra, 0, 9, round(largo_total,2)::text, round(largo_total + diferencia,2)::text, punto.gdb_from_date, punto.objectid::integer);												
							largo_total  := largo_total + diferencia;												
						END IF;
						
					END IF;

				ELSE
					SELECT TH.nombre_obra, TH.etapa, TH.tipo_obra_origen, TH.codigo_obra_origen, TH.tipo_obra_destino, TH.codigo_obra_destino, TH.longitud, TH.vigencia, TH.sistema INTO
					punto2 FROM owd.tbl_1151_hist TH WHERE TH.codigo_conduccion = codigo_obra AND TH.rut_empresa = rut;
					
					IF punto2.etapa is not null AND punto2.etapa <> etapa THEN
						INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
						VALUES (codigoproceso, codigoarchivo, rut, periodo, proyecto_informado_siss, codigo_sistema_siss, 1151, codigo_obra, 0, 3,  punto2.etapa::text, etapa::text, gdb_from_date, gdb_archive_oid);
					END IF;	
					/*IF punto2.nombre_obra is not null AND name <> punto2.nombre_obra THEN
						INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
						VALUES (codigoproceso, codigoarchivo, rut, periodo, proyecto_informado_siss, codigo_sistema_siss, 1151, codigo_obra, id_tramo_conectado, 4, punto2.nombre_obra::text, name::text, gdb_from_date, gdb_archive_oid);
					END IF;		*/
					IF punto2.tipo_obra_origen is not null AND  punto2.tipo_obra_origen <> tipo_obra_origen THEN
						INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
						VALUES (codigoproceso, codigoarchivo, rut, periodo, proyecto_informado_siss, codigo_sistema_siss, 1151, codigo_obra, 0, 5, punto2.tipo_obra_origen::text, tipo_obra_origen::text, gdb_from_date, gdb_archive_oid);
					END IF;	
					IF punto2.codigo_obra_origen is not null AND  punto2.codigo_obra_origen <> codigo_obra_origen THEN
						INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
						VALUES (codigoproceso, codigoarchivo, rut, periodo, proyecto_informado_siss, codigo_sistema_siss, 1151, codigo_obra, 0, 6, punto2.codigo_obra_origen::text, codigo_obra_origen::text, gdb_from_date, gdb_archive_oid);
					END IF;						
					IF punto2.tipo_obra_destino is not null AND  punto2.tipo_obra_destino <> tipo_obra_destino THEN
						INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
						VALUES (codigoproceso, codigoarchivo, rut, periodo, proyecto_informado_siss, codigo_sistema_siss, 1151, codigo_obra, 0, 7, punto2.tipo_obra_destino::text, tipo_obra_destino::text, gdb_from_date, gdb_archive_oid);
					END IF;	
					IF punto2.codigo_obra_destino is not null AND  punto2.codigo_obra_destino <> codigo_obra_destino THEN
						INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
						VALUES (codigoproceso, codigoarchivo, rut, periodo, proyecto_informado_siss, codigo_sistema_siss, 1151, codigo_obra, 0, 8, punto2.codigo_obra_destino::text, codigo_obra_destino::text, gdb_from_date, gdb_archive_oid);
					END IF;		

					SELECT TH.longitud INTO largo_tramo_hist 
					FROM owd.tbl_1152_hist TH 
					WHERE TH.codigo_conduccion = codigo_obra 
					AND TH.codigo_tramo = id_tramo_conectado AND TH.rut_empresa = rut;			

					SELECT TT.longitud INTO largo_cond_hist 
					FROM owd.tbl_tmp_1705 TT
					WHERE TT.rut_empresa = rut AND TT.codigo_conduccion = codigo_obra;

					SELECT longitud INTO largo_cond_hist2
					FROM owd.tbl_1151_hist TH 
					WHERE TH.codigo_conduccion = codigo_obra AND TH.rut_empresa = rut;						
					
					--largo_total := 0;

					IF largo_tramo_hist IS NOT NULL AND punto.measuredlength IS NOT NULL THEN
					
						diferencia := largo_total - largo_tramo_hist;
						IF diferencia <> 0 AND largo_total IS NOT NULL AND largo_tramo_hist IS NOT NULL THEN					

							IF largo_cond_hist IS NOT NULL THEN

								largo_total  := largo_cond_hist + diferencia;

								INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
								VALUES (codigoproceso, codigoarchivo, rut, periodo, proyecto_informado_siss, codigo_sistema_siss, 1151, codigo_obra, id_tramo_conectado, 9, round(largo_cond_hist,2)::text, round(largo_cond_hist + diferencia,2)::text, gdb_from_date, gdb_archive_oid::integer);

							ELSE

								largo_total  := largo_cond_hist2 + diferencia;

								INSERT INTO owd.tabla_1705("codigoProceso", "codigoArchivo", rut, periodo, "codigoProyecto", "codigoSistema", "codigoObraTipo", "codigoObra", "codigoTramo", "codigoAtributo", "ValorAnterior", "ValorNuevo", fecha, gdb_archive_id)	
								VALUES (codigoproceso, codigoarchivo, rut, periodo, proyecto_informado_siss, codigo_sistema_siss, 1151, codigo_obra, id_tramo_conectado, 9, round(largo_cond_hist2,2)::text, round(largo_cond_hist2 + diferencia,2)::text, gdb_from_date, gdb_archive_oid::integer);						

							END IF;
						END IF;
					END IF;

				END IF;
				codigo_sistema_siss := punto.codigo_sistema_siss;
				codigo_obra := punto.codigo_obra;
				etapa := punto.etapa;
				name := punto.nombre_conduccion;
				tipo_obra_origen:= punto.tipo_obra_origen;
				codigo_obra_origen := punto.codigo_obra_origen;
				tipo_obra_destino:= punto.tipo_obra_destino;
				codigo_obra_destino	:=	punto.codigo_obra_destino;	
				id_tramo_conectado := punto.id_tramo_conectado;
				largo_tramo := punto.measuredlength;
				rut := punto.rut;

				gdb_archive_oid := punto.gdb_archive_oid;
				gdb_from_date := punto.gdb_from_date;
				proyecto_informado_siss := punto.proyecto_informado_siss;
				assetid := punto.assetid;
				gdb_from_date = punto.gdb_from_date;					
			END IF;		
		END LOOP;


 RETURN resultado;
END;
$BODY$;

ALTER FUNCTION owd.insert_1705_1151(timestamp without time zone, timestamp without time zone)
    OWNER TO owd;
